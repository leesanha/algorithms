package com.ssafy.domain;

public class Refrigerator {
	private String serialNo;
	private String name;
	private int price;
	private int quantity;
	private double volume;
	public Refrigerator(String serialNo, String name, int price, int quantity, double volume) {
		super();
		this.serialNo = serialNo;
		this.name = name;
		this.price = price;
		this.quantity = quantity;
		this.volume = volume;
	}
	
	public String toString() {
		return serialNo + "\t | " + name + "\t | " + price + "\t | " + quantity + "\t | "  + volume;
	}
}
